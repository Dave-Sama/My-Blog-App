<%- include("./partials/header") -%>
<div id="home">
	<h1>Home</h1>
	<%content.forEach((element)=>{ %>
	<div class="container-home">
		<h3 class="title"><%=element.title%></h3>
		<p class="content">
			<%=element.content.substring(0,200) + "..."%>
			<a href="/post/<%=element.title%>">Read More</a>
		</p>
		<%})%>
	</div>
</div>
<%- include("./partials/footer") -%>
